{
  "name": "lit-chat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wireit",
    "build": "wireit",
    "preview": "wireit"
  },
  "wireit": {
    "dev": {
      "command": "vite",
      "service": true
    },
    "check-types": {
      "command": "tsc --noEmit",
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": []
    },
    "build": {
      "command": "vite build",
      "dependencies": [
        "check-types"
      ],
      "files": [
        "src/**/*",
        "index.html",
        "vite.config.ts"
      ],
      "output": [
        "dist/**"
      ]
    }
  },
  "dependencies": {
    "@ag-ui/client": "^0.0.42",
    "lit": "^3.3.1"
  },
  "devDependencies": {
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "wireit": "^0.14.12"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
